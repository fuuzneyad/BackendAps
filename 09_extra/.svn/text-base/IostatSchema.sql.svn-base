/*Schema for AVG_CPU*/
CREATE TABLE AVG_CPU (
	`ENTRY_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
	`DATETIME_ID` datetime NULL DEFAULT NULL,
	`USER` DOUBLE,
	`NICE` DOUBLE,
	`SYSTEM` DOUBLE,
	`IOWAIT` DOUBLE,
	`STEAL` DOUBLE,
	`IDLE` DOUBLE
)Engine=MyIsam;
/*Schema for DEV_IO*/
CREATE TABLE DEV_IO (
	`ENTRY_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
	`DATETIME_ID` datetime NULL DEFAULT NULL,
	`DEVICE` DOUBLE,
	`TPS` DOUBLE,
	`BLK_READ_S` DOUBLE,
	`BLK_WRTN_S` DOUBLE,
	`BLK_READ` DOUBLE,
	`BLK_WRTN` DOUBLE
)Engine=MyIsam;
