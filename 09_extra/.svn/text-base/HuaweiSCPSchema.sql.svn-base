DROP TABLE IF EXISTS SCP_CAPS;
CREATE TABLE SCP_CAPS (
	`ENTRY_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
	`SOURCE_ID` varchar(100) DEFAULT '',
	`DATETIME_ID` datetime NULL DEFAULT '0000-00-00 00:00:00',
	`GRANULARITY` int(40) ,
	`NE_ID` varchar(200) DEFAULT '',
	`MO_ID` varchar(300) DEFAULT '',
	`CAPS1` DOUBLE,
	`CAPS2` DOUBLE,
	`CAPS3` DOUBLE,
	`CAPS4` DOUBLE
)Engine=MyIsam;
DROP TABLE IF EXISTS SCP_CPU;
CREATE TABLE SCP_CPU (
	`ENTRY_DATE` timestamp NULL DEFAULT CURRENT_TIMESTAMP,
	`SOURCE_ID` varchar(100) DEFAULT '',
	`DATETIME_ID` datetime NULL DEFAULT '0000-00-00 00:00:00',
	`GRANULARITY` int(40) ,
	`NE_ID` varchar(200) DEFAULT '',
	`MO_ID` varchar(300) DEFAULT '',
	`CPU` DOUBLE
)Engine=MyIsam;
